@model IEnumerable<thcp.Models.Departament> 
   @{ 
       ViewData["Title"] = "Departamento";
   }
<h2>Lista de Depatamentos</h2>
<p>
    <a asp-action="Create" class="btn btn-success">Nuevo </a>
</p>
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.DepartamentName)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.DepartamentName)</td>
                    <td>
                        Borrar|Editar|Detalles
                    </td>
                </tr>
            }
        </tbody>


    </table>
